{
  "name": "@nolimit/base-sdk",
  "version": "1.4.2",
  "description": "Base chain SDK for noLimit Foundation - Privacy-first AI, Swap, and Mixer utilities with x402 payment protocol",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./chat": {
      "import": "./dist/clients/chat.mjs",
      "require": "./dist/clients/chat.js"
    },
    "./swap": {
      "import": "./dist/clients/swap.mjs",
      "require": "./dist/clients/swap.js"
    },
    "./mixer": {
      "import": "./dist/clients/mixer.mjs",
      "require": "./dist/clients/mixer.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/clients/*.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs": "typedoc --out docs/api src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nolimit",
    "privacy",
    "ai",
    "base",
    "ethereum",
    "x402",
    "defi",
    "swap",
    "mixer",
    "uncensored",
    "llm",
    "1inch"
  ],
  "author": "noLimit Foundation <dev@nolimit.foundation>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/noLimitFDN/nolimit-base-sdk"
  },
  "homepage": "https://nolimit.foundation/integration",
  "bugs": {
    "url": "https://github.com/noLimitFDN/nolimit-base-sdk/issues"
  },
  "dependencies": {
    "x402-fetch": "^0.2.1"
  },
  "peerDependencies": {
    "ethers": "^6.0.0",
    "viem": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "ethers": {
      "optional": true
    },
    "viem": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

